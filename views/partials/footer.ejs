<script>
function passwordstate(elm) {
  var ELM_input = elm.parentNode.querySelector('input[name="password"]');
  if (ELM_input.getAttribute('type') === "password") {
    ELM_input.setAttribute('type', 'text');
  } else {
    ELM_input.setAttribute('type', 'password');
  };
};
